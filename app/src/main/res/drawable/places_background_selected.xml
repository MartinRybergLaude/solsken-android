    android:insetRight="8dp"
    android:insetTop="2dp"
    android:insetBottom="2dp">
    <layer-list>
        <item>
            <shape android:shape="rectangle">
                <solid android:color="?attr/backgroundColored"/>
                <corners android:radius="4dp"/>
            </shape>
        </item>
    </layer-list>
</inset>